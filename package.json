{
  "name": "backend-apc",
  "version": "1.0.0",
  "description": "",
  "main": "main.ts",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon ./src/main.ts",
    "prod": "dotenvx run --env-file=.env.production -- nodemon -L ./src/main.ts",
    "start": "nodemon -L ./src/main.ts",
    "build": "tsc",
    "test": "dotenvx run --env-file=.env.test -- cross-env NODE_ENV=test jest --silent --forceExit",
    "test:integration": "wait-on tcp:8080 && dotenvx run --env-file=.env.test -- cross-env NODE_ENV=test jest --silent --forceExit",
    "test:all": "npm run prod & wait-on tcp:8080 && npm run test:integration && npm run stop2",
    "test:cov": "jest --coverage --forceExit",
    "lint": "eslint . --ext .ts --quiet",
    "lint:fix": "eslint . --ext .ts --quiet --fix",
    "lint:all": "eslint . --ext .ts",
    "stop2": "npm run stop-server",
    "stop-server": "kill-port --port 8080 || :"
  },
  "author": "Emmanuel Pericon",
  "license": "ISC",
  "dependencies": {
    "@dotenvx/dotenvx": "^0.44.4",
    "@overnightjs/core": "^1.7.6",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@typescript-eslint/parser": "^7.8.0",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "concurrently": "^8.2.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-promise-router": "^4.1.1",
    "express-validator": "^7.0.1",
    "http-status-codes": "^2.3.0",
    "jet-logger": "^1.3.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.2.3",
    "morgan": "^1.10.0",
    "prom-client": "^15.1.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "wait-on": "^7.2.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.9",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "jest": "^29.7.0",
    "mockingoose": "^2.16.2",
    "nodemon": "^3.1.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
}
